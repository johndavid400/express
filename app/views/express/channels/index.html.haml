%h1.left Listing Channels
= link_to "#{fa_icon('plus', class: 'green')} New Channel".html_safe, express.new_channel_path, :class => "right btn btn-default"

%table.table.table-striped.table-bordered
  %thead
    %tr
      %th Title
      %th Slug
      %th Status
      %th

  %tbody.list
    = render 'index', list: @channels

